    "optimize": "npm run lint:fix && npm run build",
    "deploy": "npm run optimize && npm run build:analyze"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
